<%= form_for [@merchant, @item], url: merchant_dash_items_path do |f| %>

<%= f.label :name %>
<%= f.text_field :name%>

<%= f.label :description %>
<%= f.text_field :description %>

<%= f.label :price %>
<%= f.text_field :price %>

<%= f.label :image %>
<%= f.text_field :image%>

<%= f.label :inventory %>
<%= f.text_field :inventory %>
<%= f.submit %>

<% end %>
